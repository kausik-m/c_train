#include<stdio.h>
#include<math.h>
int evenlydivisible(int n)
{
    for(int i = 1; i <=20; i ++)
    {
        if(n%i != 0)
        {
            return 0;
        }
    }
    return 1;
}

int main()
{
    int i = 20;
    while(i)
    {
        if(evenlydivisible(i) == 1)
        {
            printf("number = %d\n", i);
            break;
        }
        i += 20;
    }

    return 0;
}
